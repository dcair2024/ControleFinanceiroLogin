@model ControleFinanceiroLogin.Models.Usuario

<h2>Cadastro de Usuário</h2>

<form asp-action="Cadastro">
    <input asp-for="Nome" placeholder="Nome" required />
    <input asp-for="Email" type="email" placeholder="Email" required />
    <input asp-for="SenhaHash" type="password" placeholder="Senha" required />
    <button type="submit">Cadastrar</button>
</form>
